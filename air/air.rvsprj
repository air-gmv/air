  <analyses>
    <analysis analysis-name="coverage">
      <analysis-tasks>coverage</analysis-tasks>
      <instr-profile>COV_178_DAL_B</instr-profile>
    </analysis>
  </analyses>
  <targets>
    <target target-name="sparc-gr740">
      <commandlines>
        <clean-commandline>./rvs_air/scripts/clean.bash</clean-commandline>
        <build-commandline>./rvs_air/scripts/build.bash</build-commandline>
        <working-folder>.</working-folder>
        <run-commandline>./rvs_air/scripts/run_laysim.bash</run-commandline>
      </commandlines>
      <compiler-wrapper>
        <wrapper wrapper-name="gcc_wrap">
          <path-to-original>/opt/rtems/5.1/bin/sparc-rtems5-gcc</path-to-original>
          <rsp-enabled>true</rsp-enabled>
          <path-to-linker>/opt/rtems/5.1/bin/sparc-rtems5-ld</path-to-linker>
        </wrapper>
        <cache-argument-triggers>
          <cache-argument-trigger>
            <cache-argument-trigger-arg-pattern>-c</cache-argument-trigger-arg-pattern>
          </cache-argument-trigger>
        </cache-argument-triggers>
        <rvslib-link-triggers>
          <rvslib-link-condition-state>or</rvslib-link-condition-state>
          <rvslib-link-trigger>
            <rvslib-link-trigger-arg-pattern>-o</rvslib-link-trigger-arg-pattern>
            <rvslib-link-trigger-value-pattern>**main.o</rvslib-link-trigger-value-pattern>
          </rvslib-link-trigger>
        </rvslib-link-triggers>
        <wrapper wrapper-name="ld_wrap">
          <path-to-original>/opt/rtems/5.1/bin/sparc-rtems5-ld</path-to-original>
          <rsp-enabled>true</rsp-enabled>
          <path-to-compiler>/opt/rtems/5.1/bin/sparc-rtems5-gcc</path-to-compiler>
        </wrapper>
      </compiler-wrapper>
      <instrumentation>
        <c>
          <c-extension>${{scripts-folder}}rtems.cext</c-extension>
        </c>
      </instrumentation>
      <map>
        <map-size>16384</map-size>
      </map>
    </target>
  </targets>
  <exports>
    <export export-name="unmanaged-coverage">
      <export-section>
        <metric>coverage</metric>
        <export-format>txt</export-format>
      </export-section>
      <export-section>
        <metric>coverage</metric>
        <export-format>srchtml</export-format>
        <export-file>rvs_air/results/pmk_coverage.htm</export-file>
        <split-datasets>true</split-datasets>
      </export-section>
    </export>
  </exports>
  <testframework/>
</rvsconfig>
