      <excluded-files-list>@{{${{data-folder}}excluded-files.txt}}</excluded-files-list>
      <entry-points>"at function pmk_init()"</entry-points>
      <code-injection>
        <inline-library-injection-enabled>true</inline-library-injection-enabled>
        <inline-library-locations>
          <inline-library-location inline-library-location-name="isr_handler">
            <inject-points>"in file.glob '**pmk/arch/sparc/isr.c**' at start"</inject-points>
            <target-files-pattern>*.c</target-files-pattern>
          </inline-library-location>
        </inline-library-locations>
        <manual-ipoint-injection-enabled>false</manual-ipoint-injection-enabled>
      </code-injection>
