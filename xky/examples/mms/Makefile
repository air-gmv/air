# XKY OS installation directories
export XKY_ROOT=/home/$(AIR_RUNNER_USER)/xky
export XKY_INSTALL=/home/$(AIR_RUNNER_USER)/xky/install
export XKY_PMK=$(XKY_INSTALL)/pmk
export XKY_POS=$(XKY_INSTALL)/pos
export XKY_LIBS=$(XKY_INSTALL)/libs
export XKY_TOOLS=$(XKY_INSTALL)/tools

# Makefile Include file
include $(XKY_ROOT)/Makefile.inc


# All
.PHONY : all
all:
	make -C common
	make -C master all
	make -C p1 all
	make -C p2 all
	make -C p3 all
	make -C config all

# Clean
.PHONY : clean
clean:
	make -C common clean
	make -C master clean
	make -C p1 clean
	make -C p2 clean
	make -C p3 clean
	make -C config clean

# Distclean
.PHONY : distclean
distclean: clean
	make -C config distclean
	$(XKY_TOOLS)/configurator clean

